<%= render 'shared/content_title', title: "Tickets"%>

<table class="table">
  <thead>
    <tr>
      <th scope="col">Name</th>
      <th scope="col">Status</th>
    </tr>
  </thead>
  <tbody>
    <% @tickets.each do |ticket| %>
      <%= render 'ticket', ticket: ticket %>
    <% end %>
  </tbody>
</table>

<%= link_to "New Ticket", new_ticket_path %>